<mvc:View
  controllerName="helloworld.controller.Detail"
  displayBlock="true"
  xmlns="sap.m"
  xmlns:mvc="sap.ui.core.mvc"
  xmlns:t="sap.ui.table"
  xmlns:c="sap.ui.core"
  c:require="{}"
>
  <Page
    title="{hello>name}"
  >
    <content>
      <Text
        text="{hello>name}"
      />
      <Text
        text="{hello>id}"
      />
    </content>
  </Page>
</mvc:View>
